    "@types/node": "^16.6.1",
    "@types/nodemailer": "^6.4.4",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "@types/web-push": "^3.3.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.5",
