{
  "name": "predictor-league",
  "version": "1.0.0",
  "description": "A premier league predictor league for my friends",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.ts",
    "buildCriticalCss": "sass ./src/critical.scss:./src/compiled/critical.css --style compressed --no-source-map",
    "buildLogos": "ts-node ./src/scripts/buildLogos.ts",
    "buildData": "ts-node ./src/scripts/buildData.ts",
    "buildResults": "ts-node ./src/scripts/buildResults.ts",
    "buildPredictionStats": "ts-node ./src/scripts/buildPredictionStats.ts",
    "buildPersonalTables": "ts-node ./src/scripts/buildPersonalTables.ts",
    "buildCups": "ts-node ./src/scripts/buildCups.ts",
    "prebuildClient": "npm run buildCriticalCss",
    "buildClient": "webpack --mode production --config webpack.prod.ts",
    "buildServer": "webpack --mode production --config webpack.server.ts",
    "buildServiceWorker": "webpack --mode production --config webpack.serviceworker.ts",
    "buildAll": "npm run buildLogos && npm run buildData && npm run buildResults && npm run buildPredictionStats && npm run buildPersonalTables && npm run buildCups && npm run buildClient && npm run buildServer && npm run buildServiceWorker",
    "build": "echo 'Choose your build type'",
    "serve": "node ./serverDist/index.js"
  },
  "author": "Philip Nicholls <phil@code67.com>",
  "license": "ISC",
